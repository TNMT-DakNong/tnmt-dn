{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
   "target": "esnext",
   "lib": [
    "dom",
    "dom.iterable",
    "esnext"
  ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      
    }
  },
  "include": [
    "src"
, "pdfjs-dist.d.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
